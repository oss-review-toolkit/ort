{
    "name": "@here/oss-review-toolkit-reporter",
    "description": "React-based App to visualize scan reports generated by OSS Review Toolkit",
    "version": "0.0.1",
    "license": "Apache-2.0",
    "private": true,
    "author": {
        "name": "HERE Europe B.V.",
        "url": "https://here.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/heremaps/oss-review-toolkit.git"
    },
    "dependencies": {
        "antd": "^3.10.1",
        "classnames": "^2.2.6",
        "immutable": "^3.8.2",
        "promise": "^8.0.1",
        "prop-types": "^15.6.2",
        "raf": "^3.4.0",
        "react": "^16.5.2",
        "react-dom": "^16.5.2",
        "react-redux": "^5.0.7",
        "recharts": "^1.3.4",
        "redux": "^4.0.0",
        "redux-logger": "^3.0.6",
        "redux-saga": "^0.16.2",
        "resolve": "^1.8.1",
        "scroll-into-view-if-needed": "^2.2.20",
        "spdx-correct": "^3.0.2",
        "spdx-license-list": "^4.0.0"
    },
    "scripts": {
        "analyze": "source-map-explorer build/static/js/main.*",
        "start": "node scripts/start.js",
        "build": "node scripts/build.js",
        "test": "node scripts/test.js --env=jsdom",
        "lint": "eslint ./src"
    },
    "devDependencies": {
        "autoprefixer": "7.1.6",
        "babel-core": "6.26.0",
        "babel-eslint": "^8.1.2",
        "babel-jest": "20.0.3",
        "babel-loader": "7.1.2",
        "babel-preset-react-app": "^3.1.1",
        "babel-runtime": "6.26.0",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "chalk": "1.1.3",
        "css-loader": "0.28.7",
        "dotenv": "4.0.0",
        "dotenv-expand": "4.2.0",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^17.0.0",
        "eslint-config-react-app": "^2.1.0",
        "eslint-loader": "1.9.0",
        "eslint-plugin-babel": "^5.1.0",
        "eslint-plugin-flowtype": "2.39.1",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.10.0",
        "eslint-plugin-redux-saga": "^0.9.0",
        "extract-text-webpack-plugin": "3.0.2",
        "file-loader": "1.1.5",
        "fs-extra": "^7.0.0",
        "html-webpack-inline-source-plugin": "^0.0.10",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^23.6.0",
        "object-assign": "4.1.1",
        "postcss-flexbugs-fixes": "3.2.0",
        "postcss-loader": "2.0.8",
        "react-dev-utils": "^5.0.1",
        "source-map-explorer": "^1.6.0",
        "style-loader": "0.19.0",
        "sw-precache-webpack-plugin": "0.11.4",
        "uglifyjs-webpack-plugin": "^1.3.0",
        "url-loader": "0.6.2",
        "webpack": "3.8.1",
        "webpack-dev-server": "2.9.4",
        "webpack-event-plugin": "^1.0.0",
        "webpack-manifest-plugin": "1.3.2",
        "whatwg-fetch": "2.0.3"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,mjs}"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
            "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node",
            "mjs"
        ],
        "verbose": true,
        "watch": false,
        "watchman": false
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
