{
    "name": "@here/oss-review-toolkit-reporter",
    "description": "React-based App to visualize scan reports generated by OSS Review Toolkit",
    "version": "0.0.1",
    "license": "Apache-2.0",
    "private": true,
    "author": {
        "name": "HERE Europe B.V.",
        "url": "https://here.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/heremaps/oss-review-toolkit.git"
    },
    "dependencies": {
        "antd": "^3.19.7",
        "memoize-one": "^5.0.4",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-app-polyfill": "^1.0.1",
        "react-dom": "^16.8.6",
        "react-inspector": "^3.0.2",
        "react-redux": "^7.1.0",
        "recharts": "^1.6.2",
        "redux": "^4.0.1",
        "redux-logger": "^3.0.6",
        "redux-saga": "^1.0.1",
        "resolve": "^1.11.1",
        "scroll-into-view-if-needed": "^2.2.20",
        "spdx-correct": "^3.1.0",
        "spdx-license-list": "^6.0.0"
    },
    "scripts": {
        "start": "node scripts/start.js",
        "build": "node scripts/build.js",
        "test": "node scripts/test.js",
        "lint": "eslint ./src"
    },
    "devDependencies": {
        "@babel/core": "7.2.2",
        "@svgr/webpack": "4.1.0",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.0.1",
        "babel-jest": "24.1.0",
        "babel-loader": "8.0.5",
        "babel-plugin-named-asset-import": "^0.3.0",
        "babel-preset-react-app": "^7.0.0",
        "bfj": "6.1.1",
        "case-sensitive-paths-webpack-plugin": "2.2.0",
        "chalk": "2.4.2",
        "css-loader": "2.1.0",
        "dotenv": "6.2.0",
        "dotenv-expand": "4.2.0",
        "eslint": "5.13.0",
        "eslint-config-airbnb": "17.1.0",
        "eslint-config-react-app": "^3.0.6",
        "eslint-loader": "2.1.2",
        "eslint-plugin-babel": "5.3.0",
        "eslint-plugin-flowtype": "3.2.1",
        "eslint-plugin-import": "2.16.0",
        "eslint-plugin-jsx-a11y": "6.2.1",
        "eslint-plugin-react": "7.12.4",
        "file-loader": "3.0.1",
        "fork-ts-checker-webpack-plugin-alt": "0.4.14",
        "fs-extra": "7.0.1",
        "html-webpack-inline-source-plugin": "1.0.0-beta.2",
        "html-webpack-plugin": "4.0.0-beta.5",
        "identity-obj-proxy": "3.0.0",
        "jest": "24.1.0",
        "jest-pnp-resolver": "1.1.0",
        "jest-resolve": "24.1.0",
        "mini-css-extract-plugin": "0.5.0",
        "optimize-css-assets-webpack-plugin": "5.0.1",
        "pnp-webpack-plugin": "^1.2.1",
        "postcss-flexbugs-fixes": "4.1.0",
        "postcss-loader": "3.0.0",
        "postcss-preset-env": "6.5.0",
        "postcss-safe-parser": "4.0.1",
        "react-dev-utils": "^7.0.1",
        "sass-loader": "7.1.0",
        "style-loader": "0.23.1",
        "terser-webpack-plugin": "1.2.2",
        "typescript": "^3.3.3",
        "url-loader": "1.1.2",
        "webpack": "4.29.3",
        "webpack-dev-server": "3.1.14",
        "webpack-event-plugin": "^1.1.1",
        "webpack-manifest-plugin": "2.0.4",
        "workbox-webpack-plugin": "3.6.3"
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "eslintConfig": {
        "extends": "react-app"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "resolver": "jest-pnp-resolver",
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jsdom",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "verbose": true,
        "watch": false,
        "watchman": false
    },
    "homepage": "."
}
