{
    "name": "@here/oss-review-toolkit-reporter",
    "description": "React-based App to visualize scan reports generated by OSS Review Toolkit",
    "version": "0.2.0",
    "license": "Apache-2.0",
    "private": true,
    "author": {
        "name": "HERE Europe B.V.",
        "url": "https://here.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/oss-review-toolkit/ort.git",
        "directory": "reporter-web-app"
    },
    "dependencies": {
        "@ant-design/icons": "^4.0.5",
        "antd": "^4.1.1",
        "markdown-to-jsx": "^6.11.1",
        "memoize-one": "^5.1.1",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-app-polyfill": "^1.0.6",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.0",
        "react-syntax-highlighter": "^12.2.1",
        "recharts": "2.0.0-beta.2",
        "redux": "^4.0.5",
        "redux-logger": "^3.0.6",
        "redux-saga": "^1.1.3",
        "resolve": "^1.15.1",
        "scroll-into-view-if-needed": "^2.2.24",
        "spdx-correct": "^3.1.0",
        "spdx-license-list": "^6.1.0"
    },
    "scripts": {
        "start": "node scripts/start.js",
        "build": "node scripts/build.js",
        "test": "node scripts/test.js",
        "lint": "eslint ./src"
    },
    "devDependencies": {
        "@babel/core": "7.9.0",
        "@svgr/webpack": "5.3.1",
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "babel-eslint": "10.1.0",
        "babel-jest": "25.2.6",
        "babel-loader": "8.1.0",
        "babel-plugin-named-asset-import": "^0.3.6",
        "babel-preset-react-app": "^9.1.2",
        "case-sensitive-paths-webpack-plugin": "2.3.0",
        "chalk": "4.0.0",
        "css-loader": "3.5.1",
        "dotenv": "8.2.0",
        "dotenv-expand": "5.1.0",
        "eslint": "6.8.0",
        "eslint-config-airbnb": "18.1.0",
        "eslint-config-react-app": "^5.2.1",
        "eslint-loader": "4.0.0",
        "eslint-plugin-babel": "5.3.0",
        "eslint-plugin-flowtype": "4.7.0",
        "eslint-plugin-import": "2.20.2",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-react": "7.19.0",
        "eslint-plugin-react-hooks": "3.0.0",
        "file-loader": "6.0.0",
        "fork-ts-checker-webpack-plugin-alt": "0.4.14",
        "fs-extra": "9.0.0",
        "html-webpack-inline-source-plugin": "1.0.0-beta.2",
        "html-webpack-plugin": "4.0.4",
        "identity-obj-proxy": "3.0.0",
        "jest": "25.2.7",
        "jest-pnp-resolver": "1.2.1",
        "jest-resolve": "25.2.6",
        "jest-watch-typeahead": "^0.5.0",
        "mini-css-extract-plugin": "0.9.0",
        "optimize-css-assets-webpack-plugin": "5.0.3",
        "pnp-webpack-plugin": "^1.6.4",
        "postcss-flexbugs-fixes": "4.2.0",
        "postcss-loader": "3.0.0",
        "postcss-normalize": "^8.0.1",
        "postcss-preset-env": "6.7.0",
        "postcss-safe-parser": "4.0.2",
        "react-dev-utils": "^10.2.1",
        "resolve-url-loader": "^3.1.1",
        "sass-loader": "8.0.2",
        "style-loader": "1.1.3",
        "terser-webpack-plugin": "2.3.5",
        "typescript": "^3.8.3",
        "url-loader": "4.0.0",
        "webpack": "4.42.1",
        "webpack-dev-server": "3.10.3",
        "webpack-event-plugin": "^1.1.1",
        "webpack-manifest-plugin": "2.2.0",
        "workbox-webpack-plugin": "4.3.1"
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "eslintConfig": {
        "extends": "react-app"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "resolver": "jest-pnp-resolver",
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jsdom",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "verbose": true,
        "watch": false,
        "watchman": false
    },
    "homepage": "."
}
